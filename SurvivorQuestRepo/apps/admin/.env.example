# Data source mode
# true  -> use local Next.js route handlers under /api/* (mock mode)
# false -> use external backend via NEXT_PUBLIC_API_URL
NEXT_PUBLIC_USE_MOCK_API=true

# External backend base URL (used when NEXT_PUBLIC_USE_MOCK_API=false)
# Example: https://api.survivorquest.app
NEXT_PUBLIC_API_URL=
